{
  "name": "jovo-starter-web-standalone-app",
  "version": "3.0.0",
  "description": "A sample voice app that works with the Jovo Framework",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "dependencies": {
    "jovo-db-filedb": "^3.1.4",
    "jovo-slu-lex": "^3.1.4",
    "jovo-tts-polly": "^3.1.4",
    "jovo-framework": "^3.1.4",
    "jovo-platform-web": "^3.1.4",
    "jovo-plugin-debugger": "^3.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
    "@types/socket.io-client": "^1.4.32",
    "gulp": "^4.0.2",
    "gulp-install": "^1.1.0",
    "gulp-run-command": "0.0.10",
    "gulp-zip": "^5.0.1",
    "jest": "^26.0.1",
    "ts-jest": "^25.5.0",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "tscw": "tsc --watch",
    "start:dev": "tsc-watch --onSuccess \"jovo run --timeout 60000\"",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "bundle": "gulp --gulpfile node_modules/jovo-framework/gulpfile.js build-ts --cwd ./",
    "start": "cd ./dist && node index.js --webhook",
    "launch": "npm start -- --launch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovotech/jovo-sample-voice-app-nodejs.git"
  },
  "author": "Jovo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jovotech/jovo-sample-voice-app-nodejs/issues"
  },
  "homepage": "https://github.com/jovotech/jovo-sample-voice-app-nodejs#readme"
}
