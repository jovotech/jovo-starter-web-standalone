{
  "invocation": "my test app",
  "version": "4.0",
  "intents": {
    "SwitchThemeIntent": {
      "phrases": [
        "{theme}",
        "switch to {theme} mode",
        "{theme} mode",
        "enable {theme} mode",
        "{theme} theme",
        "switch to the {theme} theme",
        "i want the {theme} theme"
      ],
      "entities": {
        "theme": {
          "type": "ThemeType"
        }
      }
    }
  },
  "entityTypes": {
    "ThemeType": {
      "values": [
        {
          "value": "light"
        },
        { "value": "dark" }
      ]
    }
  }
}
